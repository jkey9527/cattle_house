<view class="container" style="height: 100%;">
  <view class="top">个人信息</view>
  <view class="formItem">
    <view class="item">
      <view class="label">编号</view>
      <view class="value">
        <input class="weui-input" disabled="true" value="{{user.user_no}}" />
      </view>
    </view>
    <view class="item">
      <view class="label">名称</view>
      <view class="value">
        <input class="weui-input" disabled="true" value="{{user.user_name}}" />
      </view>
    </view>
    <view class="item">
      <view class="label">手机号</view>
      <view class="value">
        <input class="weui-input" disabled="true" value="{{user.user_phone}}" />
      </view>
    </view>
    <view class="item">
      <view class="label">身份证号</view>
      <view class="value">
        <input class="weui-input" disabled="true" value="{{user.user_id_card}}" />
      </view>
    </view>
    <view class="item">
      <view class="label">车牌号</view>
      <view class="value">
        <input class="weui-input" disabled="true" value="{{user.user_car_no}}" />
      </view>
    </view>
    <view class="item">
      <view class="label">备注</view>
      <view class="value">
        <input class="weui-input" disabled="true" value="{{user.user_notes}}" />
      </view>
    </view>
    <view class="item">
      <view class="label">密码锁编号</view>
      <view class="value">
        <input class="weui-input" disabled="true" value="{{user.user_key_no}}" />
      </view>
    </view>
    <view class="weui-form__opr-area login_out">
      <a role="button" disabled="" aria-disabled="true" class="weui-btn weui-btn_primary" href="javascript" id="showTooltips" wah-hotarea="click" bindtap="loginOut">退出登录</a>
    </view>
  </view>
</view>