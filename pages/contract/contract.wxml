<view class="container" style="height: 100%;">
  <view class="top">合同信息</view>
    <view class="formItem">
      <view class="item">
        <view class="label">合同编号</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_no}}" />
        </view>
      </view>
      <view class="item">
        <view class="label">开始日期</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_start_date}}" />
        </view>
      </view>
      <view class="item">
        <view class="label">结束日期</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_end_date}}" />
        </view>
      </view>
      <view class="item">
        <view class="label">合同期限</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_year}}" />年
        </view>
      </view>
      <view class="item">
        <view class="label">房屋类型</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_house_type===2?'套二':'套一'}}" />
        </view>
      </view>
      <view class="item">
        <view class="label">房间号</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_house_no}}" />
        </view>
      </view>
      <view class="item">
        <view class="label">每月租金</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_month_money}}" />元
        </view>
      </view>
      <view class="item">
        <view class="label">押金</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_deposit_money}}" />元
        </view>
      </view>
      <view class="item">
        <view class="label">已付租金</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_paid_money}}" />元
        </view>
      </view>
      <view class="item">
        <view class="label">待付租金</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_pay_money}}" />元
        </view>
      </view>
      <view class="item">
        <view class="label">总租金</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_total_money}}" />元
        </view>
      </view>
      <view class="item">
        <view class="label">钥匙数</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_key_num}}" />把
        </view>
      </view>
      <view class="item">
        <view class="label">水单价</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_w_price}}" />元
        </view>
      </view>
      <view class="item">
        <view class="label">电单价</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_e_price}}" />元
        </view>
      </view>
      <view class="item">
        <view class="label">气单价</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_g_price}}" />元
        </view>
      </view>
      <view class="item">
        <view class="label">水期初</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_w_start}}" />吨
        </view>
      </view>
      <view class="item">
        <view class="label">电期初</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_e_start}}" />度
        </view>
      </view>
      <view class="item">
        <view class="label">气期初</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_g_start}}" />方
        </view>
      </view>
      <view class="item">
        <view class="label">合同状态</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_state===1?'在租':'待租'}}" />
        </view>
      </view>
      <view class="item">
        <view class="label">备注</view>
        <view class="value">
          <input class="weui-input" disabled="true" value="{{list.con_notes}}" />
        </view>
      </view>
    </view>
</view>